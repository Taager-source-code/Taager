import { Component, Input } from '@angular/core';

@Component({

  selector: 'app-image-component',

  templateUrl: './image-component.component.html',

  styleUrls: ['./image-component.component.scss']

})

export class SharedImageComponent {

  @Input() url: string;

  @Input() width: string;

  @Input() height: string;

  @Input() styleInput: string;

  @Input() altInput = '';

  @Input() class: string;

  @Input() title = '';

  constructor() { }

}
